{
    "name": "pariaza-inteligent",
    "version": "1.0.0",
    "private": true,
    "description": "Production-ready value betting platform with immersive 3D UI and double-entry ledger",
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "test": "turbo run test",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
        "db:migrate": "cd packages/database && pnpm prisma migrate dev",
        "db:seed": "cd packages/database && pnpm prisma db seed",
        "db:studio": "cd packages/database && pnpm prisma studio"
    },
    "devDependencies": {
        "@turbo/gen": "^2.3.3",
        "pino-pretty": "^13.1.3",
        "prettier": "^3.4.2",
        "prisma": "^5.22.0",
        "turbo": "^2.3.3",
        "typescript": "^5.7.2"
    },
    "engines": {
        "node": ">=20.0.0",
        "pnpm": ">=9.0.0"
    },
    "packageManager": "pnpm@9.15.0",
    "dependencies": {
        "dotenv": "^16.4.7",
        "mysql2": "^3.16.0"
    }
}